{
  "name": "next-js-boilerplate",
  "version": "1.0.0",
  "engines": {
    "node": "16.20.0"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "build-stats": "ANALYZE=true npm run build",
    "newpost": "node generatePost.config.js",
    "export": "next export -o dist",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next dist",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "deployy": "aws s3 sync ./dist s3://cativosaverdade.com --profile=cativosaverdade",
    "deploy": "aws s3 sync ./dist s3://cativosaverdade.com --acl=public-read --profile=cativosaverdade && aws cloudfront create-invalidation --profile=cativosaverdade --distribution-id E3PJNYFZLECHPC --paths '/*'",
    "build&upload": "run-s clean build export && aws s3 sync ./dist s3://cativosaverdade.com --acl=public-read"
  },
  "dependencies": {
    "@babel/plugin-transform-typescript": "^7.22.9",
    "@mapbox/rehype-prism": "^0.5.0",
    "@types/react": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "caniuse-lite": "^1.0.30001519",
    "date-fns": "^2.16.1",
    "gray-matter": "^4.0.2",
    "next": "^13.4.12",
    "next-seo": "^4.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^8.0.0",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "slugify": "^1.4.6",
    "styled-jsx-plugin-postcss": "^4.0.0",
    "typescript": "^5.1.6",
    "unified": "^9.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.4",
    "@types/node": "^14.14.20",
    "autoprefixer": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "eslint"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "license": "ISC"
}
